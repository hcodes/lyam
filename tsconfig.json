{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "lib": [
            "es6",
            "dom"
        ],
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "forceConsistentCasingInFileNames": true
    }
}
